{
    "query1": {"WHERE": {"IS": {"courses_dept": "cpsc"} }, "OPTIONS": {"COLUMNS": ["courses_dept", "courses_id"], "ORDER": "courses_id"} },
    "query2": {
        "WHERE": {
            "OR": [
                {
                    "NOT": {
                        "EQ": {
                            "courses_audit": 10
                        }
                    }
                },
                {
                    "EQ": {
                        "courses_avg": 90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_audit",
                "courses_instructor"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "courses_avg",
                    "courses_fail"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_avg",
                "courses_year"
            ],
            "APPLY": [
                {
                    "overallAvg": {
                        "AVG": "courses_avg"
                    }
                },
                {
                    "max": {
                        "MAX": "courses_audit"
                    }
                }
            ]
        }
    }
}
